{
	"name": "slexphp/json-serialization",
	"repositories": [{
		"type": "git",
		"url": "https://github.com/slepic/slexphp-serialization-contracts.git"
	}],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"slexphp/serialization-contracts": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Slexphp\\Serialization\\Json\\": "src"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"squizlabs/php_codesniffer": "^3.5",
		"vimeo/psalm": "^3.12"
	}
}
